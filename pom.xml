                        </modules>
                        -->
                    </configuration>
                    <goals>
                        <goal>update-widgetset</goal>
                    </goals>
                </execution>
            </executions>
        </plugin>
  <plugin>
    <groupId>org.apache.felix</groupId>
   <artifactId>maven-bundle-plugin</artifactId>
   <version>2.1.0</version>
   <extensions>true</extensions>
   <configuration>
    <excludeDependencies>${wits.osgi.exclude.dependencies}</excludeDependencies>
    <instructions>
     <Bundle-Name>${project.name}</Bundle-Name>
     <Bundle-SymbolicName>${wits.osgi.symbolic.name}</Bundle-SymbolicName>
     <Bundle-Activator>${wits.osgi.activator}</Bundle-Activator>
     <Export-Package>${wits.osgi.export}</Export-Package>
     <Import-Package>org.wits.osgi;resolution:=optional, ${wits.osgi.import}</Import-Package>
     <DynamicImport-Package>${wits.osgi.dynamic}</DynamicImport-Package>
     <Private-Package>${wits.osgi.private.pkg}</Private-Package>
     <Implementation-Title>WITS</Implementation-Title>
     <Implementation-Version>${project.version}</Implementation-Version>
     <Include-Resource>${wits.osgi.include.resource}</Include-Resource>
     <_versionpolicy>${wits.osgi.import.default.version}</_versionpolicy>
     <_removeheaders>${wits.osgi.remove.headers}</_removeheaders>
     <_failok>${wits.osgi.failok}</_failok>
     <Export-Service>${wits.osgi.export.service}</Export-Service>
     <Import-Service>${wits.osgi.import.service}</Import-Service>
     <Embed-Dependency>${wits.osgi.embed}</Embed-Dependency>
    </instructions>
    <versions>
     <wits.osgi.version.clean>${project.version}</wits.osgi.version.clean>
    </versions>
   </configuration>
         <executions>
          <execution>
            <phase>generate-sources</phase>
            <goals>
              <goal>cleanVersions</goal>
            </goals>
          </execution>
          <execution>
            <id>bundle-manifest</id>
            <phase>process-classes</phase>
            <goals>
              <goal>manifest</goal>
            </goals>
          </execution>
        </executions>
  </plugin>  
    <plugin>
