    <wits.osgi.import>${wits.osgi.import.pkg}</wits.osgi.import>
    <wits.osgi.dynamic />
    <wits.osgi.symbolic.name>${wits.artifact}</wits.osgi.symbolic.name>
    <wits.osgi.exclude.dependencies>false</wits.osgi.exclude.dependencies>
    <wits.osgi.remove.headers>Ignore-Package,Include-Resource,Private-Package,Bundle-DocURL</wits.osgi.remove.headers>
