                		<goals>
                			<goal>resources</goal>
                		</goals>
                	</pluginExecutionFilter>
                	<action>
                		<ignore></ignore>
                	</action>
                </pluginExecution>
                <pluginExecution>
                	<pluginExecutionFilter>
                		<groupId>com.vaadin</groupId>
                		<artifactId>vaadin-maven-plugin</artifactId>
                		<versionRange>[1.0.1,)</versionRange>
                		<goals>
                			<goal>update-widgetset</goal>
                		</goals>
                	</pluginExecutionFilter>
                	<action>
                		<ignore></ignore>
                	</action>
                </pluginExecution>
              </pluginExecutions>
            </lifecycleMappingMetadata>
          </configuration>
        </plugin>
